<Window x:Class="WebParserData.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WebParserData"
        mc:Ignorable="d"
        Title="MainWindow" Height="375" Width="700">
    <Grid>
        <Ellipse Fill="Red" HorizontalAlignment="Left" Height="40" Margin="-17,-15,0,0" Stroke="Black" VerticalAlignment="Top" Width="40"/>
        <Button Name="ButtonStart" Content="Start" HorizontalAlignment="Left" Margin="10,309,0,0" VerticalAlignment="Top" Width="100" Height="25" Click="ButtonStart_Click"/>
        <Button Name="ButtonStop"  Content="Stop" HorizontalAlignment="Left" Margin="115,309,0,0" VerticalAlignment="Top" Width="100" Height="25" Click="ButtonStop_Click"/>
        <Image HorizontalAlignment="Left" Height="175" Margin="10,30,0,0" VerticalAlignment="Top" Width="175" Source="Resources/no_image.png"/>
        <RichTextBox HorizontalAlignment="Left" Height="324" Margin="222,10,0,0" VerticalAlignment="Top" Width="460">
            <FlowDocument>
                <Paragraph>
                    <Run Text="RichTextBox"/>
                </Paragraph>
            </FlowDocument>
        </RichTextBox>

    </Grid>
</Window>
